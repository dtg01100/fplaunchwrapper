# fplaunchwrapper configuration example
# Copy this file to default.toml and customize for your system

schema_version = 1
# Directory where wrapper scripts will be generated
# Common locations: ~/.local/bin, ~/bin
bin_dir = "~/.local/bin"
debug_mode = false
log_level = "INFO"
blocklist = []
cron_interval = 6
enable_notifications = true
hook_failure_mode_default = "warn"

[global_preferences]
launch_method = "auto"
custom_args = []

[global_preferences.env_vars]
